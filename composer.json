{
    "name": "piteurstudio/php-nicdz",
    "description": "checks availability of .DZ and whois data via the nic.dz API",
    "keywords": [
        "PiteurStudio",
        "php-domain-dz",
        "nic.dz",
        "Algeria"
    ],
    "homepage": "https://github.com/piteurstudio/php-domain-dz",
    "license": "MIT",
    "authors": [
        {
            "name": "Nassim",
            "email": "mb.nassim@piteur-studio.dz",
            "homepage": "https://www.piteur-studio.dz"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/http-client": "^6.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.4",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "PiteurStudio\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PiteurStudio\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
